.header{ width:960px; height:100px; margin:0 auto;}
.header .logo{ width:185px; height:50px; float:left; margin-top:25px; background:url(../images/Logo_ZhuChe.png);}



.container{ width:960px; margin:0 auto; padding:20px 0; -moz-box-shadow:2px 2px 10px #bbb; -webkit-box-shadow:2px 2px 10px #bbb; box-shadow:2px 2px 10px #bbb;}
.container .process{ width:100%; height:35px; background:url(../images/jindu_2.jpg) top left no-repeat;}
.container form{ float:left; width:100%;}
.container form label{ height:50px; line-height:50px;}
.container form label .input_title{ width:200px; float:left; text-align:right;}
.container form label input{ width:200px; height:24px; padding:3px 5px; line-height:24px; float:left; margin:10px 0 0 20px; }



.footer{ }